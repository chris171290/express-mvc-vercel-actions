{
  "name": "express-mvc-vercel-actions",
  "version": "1.0.0",
  "description": "Rest Api - MVC pattern",
  "type": "module",
  "main": "server-with-mysql.js",
  "scripts": {
    "start": "node ./src/server-with-mysql.js",
