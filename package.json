    "swagger-ui-express": "4.6.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-async": "2.0.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "servor": "4.0.2",
    "standart": "6.1.0",
    "supertest": "6.3.4"
